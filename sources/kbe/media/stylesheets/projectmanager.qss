    QTreeView::branch:has-siblings:!adjoins-item  {
        border-image: url(:/media/icons/pm-vline.png) 0;
    }

    QTreeView::branch:has-siblings:adjoins-item  {
        border-image: url(:/media/icons/pm-branch-more.png) 0;
    }

    QTreeView::branch:!has-children:!has-siblings:adjoins-item  {
        border-image: url(:/media/icons/pm-branch-end.png) 0;
    }

    QTreeView::branch:has-children:!has-siblings:closed,
    QTreeView::branch:closed:has-children:has-siblings  {
            border-image: none;
            image: url(:/media/icons/pm-branch-closed.png);
    }

    QTreeView::branch:open:has-children:!has-siblings,
    QTreeView::branch:open:has-children:has-siblings   {
            border-image: none;
            image: url(:/media/icons/pm-branch-open.png);
    }
